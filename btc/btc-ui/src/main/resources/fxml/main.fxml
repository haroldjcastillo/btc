<BorderPane xmlns="http://javafx.com/javafx/8.0.91" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.haroldjcastillo.btc.ui.MainController">
	<top>
		<Pane>
			<fx:include fx:id="header" source="header.fxml" />
