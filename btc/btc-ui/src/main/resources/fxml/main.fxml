<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.haroldjcastillo.btc.ui.MainController">
	<top>
		<Pane>
			<fx:include fx:id="header" source="header.fxml" />
		</Pane>
	</top>
	<left>
		<Pane>
			<fx:include fx:id="recent" source="recent.fxml" />
		</Pane>
	</left>
	<center>
		<Pane>
			<fx:include fx:id="orders" source="orders.fxml" />
   		<Pane>
   			<fx:include fx:id="ticks" source="ticks.fxml" />
   		</Pane>
		</Pane>
	</center>
	<right>
		<Pane>
			<fx:include fx:id="trade" source="right.fxml" />
		</Pane>
	</right>

</BorderPane>
